# fizzbuzz
defmodule FizzBuzz do
  def run(n) do
    for i <- 1..n do
      cond do
        rem(i, 15) == 0 -> IO.puts("FizzBuzz")
        rem(i, 3) == 0 -> IO.puts("Fizz")
        rem(i, 5) == 0 -> IO.puts("Buzz")
        true -> IO.puts(Integer.to_string(i))
      end
    end
  end
end

FizzBuzz.run(20)
